<!DOCTYPE html>
<html lang="en">
<body>
<header>
    <div class="container">
        <div class="row">
            <div class="col-md-4">
                <div class="eny">
                    <div class="avatar">
                        
                        <img src="https://cdn.discordapp.com/avatars/<%= user.id %>/<%= user.avatar %>.png" alt="">
                    </div>
                    <div class="ent">
                        <h3><%= user.username %>#<%= user.discriminator %></h3>

                        <%= 
                            let roles = bot.guilds.cache.get('700208007530676314').members.cache.get(user.id).roles


                            for (var i = 0; i < roles.length; i++) {
                                var role1 = roles[i]
                                var role2 = roles[i]
                            }

                        %>

                        <span><i class="fas fa-circle"></i> <%= role1 %></span>
                        <span><i class="fas fa-circle"></i> <%= role2 %></span>
                    </div>
                </div>
            </div>
            <div class="col-md-4 text-center">
                <div class="logo">
                    <img src="public/images/logo.png" alt="">
                </div>
            </div>
            <div class="col-md-4 text-right">
                <div class="coin">
                    <div class="avatar">
                        <img style="width: 40px" src="public/images/coin.png" alt="">
                    </div>
                    <div class="ent">
                        132, 103
                    </div>
                </div>
            </div>
        </div>
    </div>
</header>
